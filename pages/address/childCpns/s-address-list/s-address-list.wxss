.address-list {
  box-sizing: border-box;
  width: 100%;
  height:auto;
  border: 0px solid red;
  padding: 25rpx 16rpx;
}

.item-warp {
  border-bottom: 1px solid #cccccc;
  margin-bottom: 20rpx;
}

.item-warp:last-child {
  border-bottom: none;
  margin-bottom: 0;
}


.address-list-bootom {
  border-top: 1px solid #cccccc;
  margin-top: 30rpx;
  height: 10%;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0rpx;
}

.address-list-bootom > text {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width: 350rpx;
  font-size: 30rpx;
  height: 65rpx;
  border-radius: 32.5rpx;
  color: #ffffff;
  background-color: red;
  border: 1px solid red;

}
